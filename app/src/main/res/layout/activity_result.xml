<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainmenu_bg"
    android:padding="10dp"
    tools:context="readwithme.com.ResultActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="430dp"
            android:layout_height="300dp"
            android:layout_centerInParent="true"
            android:background="@drawable/quiz_bg">

            <LinearLayout
                android:id="@+id/star_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:orientation="horizontal"
                android:layout_marginLeft="5dp"
                android:padding="2dp">


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_alignParentBottom="true"
                android:orientation="horizontal"
                android:paddingLeft="170dp">

                <Button
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:background="@drawable/button_retry"
                    android:onClick="cmd_result_Clicked"
                    android:tag="1" />

                <Button
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:background="@drawable/button_menu_result"
                    android:onClick="cmd_result_Clicked"
                    android:tag="2" />
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>



